
public class Solution68644 {
	public static int numbers[] = {2, 1, 3, 4, 1}; // 임시 지워
	public static int len, count;
	public static boolean vtd[];
	public static int answer[] = {};
	
	public static void solve(int idx, int cnt, int sum) {
		if(cnt == 2) {
			answer[count++] = sum;
			return;
		}
		
		for(int i = 0; i < len; i++) {
			if(!vtd[i]) {
				vtd[i] = true;
				solve(i, cnt+1, sum+=numbers[i]);
				vtd[i] = false;
			}
		}
	}
	
	public static void main(String[] args) {
		len = numbers.length;
		count = 0;
		vtd = new boolean[len];
		solve(0, 0, 0);
		
	}
}
